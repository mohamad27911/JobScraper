{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "backend/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      },
      {
        "src": "/jobs/(.*)",
        "dest": "/backend/main.py"
      }
    ]
  }
  